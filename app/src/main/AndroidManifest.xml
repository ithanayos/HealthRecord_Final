<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="utcc.som.cken.tae.healthrecord">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo4"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RecordActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".TestActivity" />
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".DiseaseActivity"
            android:label="@string/title_disease"
            android:theme="@style/AppTheme" />
        <activity android:name=".HeartActivity" />
        <activity android:name=".diabetesActivity" />
        <activity android:name=".LowpressureActivity" />
        <activity android:name=".highpressureActivity" />
        <activity android:name=".CheckhealthActivity" />
        <activity android:name=".AboutmeActivity" />
        <activity android:name=".CheckpressActivity" />
        <activity android:name=".CheckheartActivity" />
        <activity android:name=".CheckglucoseActivity" />
        <activity android:name=".CheckbmiActivity"></activity>
    </application>

</manifest>
